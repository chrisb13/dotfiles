# ssh/config
Host *
    ForwardAgent yes
    ForwardX11 yes

Host garrett.unn.ac.uk
    StrictHostKeyChecking no
    UserKnownHostsFile=/dev/null

Host oswald
    User osw_lhly6
    IdentityFile /home/chris/.ssh/id_rsa
    HostName oswald.campus.unn.ac.uk
    ProxyCommand ssh -W %h:%p -p 4219 prx_lhly6@garrett.unn.ac.uk 2> /dev/null

Host oswaldvis
    User osw_lhly6
    IdentityFile /home/chris/.ssh/id_rsa
    HostName oswald-vis1.campus.unn.ac.uk
    ProxyCommand ssh -W %h:%p -p 4219 prx_lhly6@garrett.unn.ac.uk 2> /dev/null


Host bhop
  User chbull
  HostName bslcenc.nerc-bas.ac.uk

Host jass
  User chbull
  HostName jasmin-login1.ceda.ac.uk
  ForwardAgent yes
  IdentityFile /users/chbull/.ssh/id_rsa_jasmin
  ProxyJump bhop


Host C
 User chbull
 HostName jasmin-login1.ceda.ac.uk
 ProxyCommand ssh -T -q -o 'ForwardAgent yes' bhop #'ssh-add -t 1 /users/chbull/.ssh/id_rsa_jasmin '

